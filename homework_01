#程序
company_name = input("请输入公司名称：")#*****
name = input("请输入姓名：")            #***
phone_no = input("请输入电话：")        #******2
email = input("请输入邮箱：")           #****@163.com

print("*"*50)
print("%s" % company_name)
print()
print("{}".format(name))
print()
print('电话：{}'.format(phone_no))
print("邮箱：{}".format(email))
print("*"*50)

#打印结果如下
#**************************************************
#*****
#
#***
#
#电话：******2
#邮箱：****@163.com
#**************************************************

