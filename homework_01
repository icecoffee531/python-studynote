#程序
company_name = input("请输入公司名称：")#武昌首义学院
name = input("请输入姓名：")#杨辰瑞
phone_no = input("请输入电话：")#18617280612
email = input("请输入邮箱：")#2335250710@qq.com

print("*"*50)
print("%s" % company_name)
print()
print("{}".format(name))
print()
print('电话：{}'.format(phone_no))
print("邮箱：{}".format(email))
print("*"*50)

#打印结果如下
#**************************************************
#武昌首义学院
#
#杨辰瑞
#
#电话：18617280612
#邮箱：2335250710@qq.com
#**************************************************

